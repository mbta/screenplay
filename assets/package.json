{
  "repository": {},
  "description": " ",
  "license": "MIT",
  "scripts": {
    "deploy": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "lint": "eslint --fix \"{.,**}/*.{js,json,ts,tsx}\"",
    "lint:check": "eslint \"{.,**}/*.{js,json,ts,tsx}\"",
    "format": "prettier --write \"{.,**}/*.{js,json,ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"{.,**}/*.{js,json,ts,tsx,css,scss}\"",
    "test": "jest",
    "check": "npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "@fullstory/browser": "^1.6.2",
    "@heroicons/react": "^1.0.4",
    "@sentry/react": "^7.14.0",
    "bootstrap": "^5.1.3",
    "date-fns": "^2.23.0",
    "moment": "^2.29.4",
    "phoenix": "file:../deps/phoenix",
    "phoenix_html": "file:../deps/phoenix_html",
    "react": "^17.0.2",
    "react-bootstrap": "^2.4.0",
    "react-bootstrap-icons": "^1.8.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "react-tooltip": "^4.2.21"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^27.5.1",
    "@types/phoenix": "^1.5.2",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^8.15.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "file-loader": "^6.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "2.3.2",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^3.0.0",
    "terser-webpack-plugin": "^2.3.2",
    "ts-jest": "^28.0.2",
    "ts-loader": "^8.2.0",
    "typescript": "^4.6.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.2"
  },
  "jest": {
    "clearMocks": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(js|tests)/.*\\.test\\.tsx?$",
    "modulePaths": [
      "<rootDir>/js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "^.+\\.(css|less|scss)$": "babel-jest"
    },
    "collectCoverage": true,
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.test.json"
      }
    },
    "globalSetup": "./tests/global-setup.js",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ]
  }
}
