{
  "repository": {},
  "description": " ",
  "license": "MIT",
  "scripts": {
    "deploy": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "lint": "eslint --fix \"{.,**}/*.{js,ts,tsx}\"",
    "lint:check": "eslint \"{.,**}/*.{js,ts,tsx}\"",
    "format": "prettier --write \"{.,**}/*.{js,json,ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"{.,**}/*.{js,json,ts,tsx,css,scss}\"",
    "test": "jest",
    "check": "npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "@fullstory/browser": "^2.0.5",
    "@heroicons/react": "^2.1.5",
    "@sentry/fullstory": "^3.0.0",
    "@sentry/react": "^8.13.0",
    "@types/pluralize": "^0.0.33",
    "bootstrap": "^5.1.3",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.45",
    "pluralize": "^8.0.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-bootstrap-icons": "^1.8.2",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.3.0",
    "react-search-autocomplete": "^8.5.2",
    "react-tooltip": "^4.2.21",
    "regenerator-runtime": "^0.14.1",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.1",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.9",
    "@types/phoenix": "^1.6.5",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^8.15.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "file-loader": "^6.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.1",
    "prettier": "3.2.5",
    "react-test-renderer": "^18.3.1",
    "sass": "^1.69.5",
    "sass-loader": "^16.0.2",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "clearMocks": true,
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        "./tsconfig.test.json"
      ]
    },
    "testRegex": "(js|tests)/.*\\.test\\.tsx?$",
    "modulePaths": [
      "<rootDir>/js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "^.+\\.(css|less|scss)$": "babel-jest",
      "^Components(.*)$": "<rootDir>/js/components/Dashboard$1",
      "^Hooks(.*)$": "<rootDir>/js/hooks$1",
      "^Models(.*)$": "<rootDir>/js/models$1",
      "^Utils(.*)$": "<rootDir>/js/utils$1",
      "^Constants(.*)$": "<rootDir>/js/constants$1"
    },
    "collectCoverage": true,
    "globalSetup": "./tests/global-setup.js",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ]
  }
}
