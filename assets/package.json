{
  "repository": {},
  "description": " ",
  "license": "MIT",
  "scripts": {
    "deploy": "node build.mjs deploy",
    "lint": "eslint --fix \"{.,**}/*.{js,ts,tsx}\"",
    "lint:check": "eslint \"{.,**}/*.{js,ts,tsx}\"",
    "format": "prettier --write \"{.,**}/*.{js,mjs,json,ts,tsx,css,scss}\" --list-different",
    "format:check": "prettier --check \"{.,**}/*.{js,mjs,json,ts,tsx,css,scss}\"",
    "test": "jest",
    "check": "npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "@fullstory/browser": "^2.0.6",
    "@heroicons/react": "^2.1.5",
    "@sentry/fullstory": "^5.0.0",
    "@sentry/react": "^8.34.0",
    "@types/pluralize": "^0.0.33",
    "bootstrap": "^5.3.8",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.48",
    "pluralize": "^8.0.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.3.0",
    "react-search-autocomplete": "^8.5.2",
    "react-tooltip": "^4.2.21",
    "regenerator-runtime": "^0.14.1",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.1",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.21",
    "@types/phoenix": "^1.6.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "esbuild": "^0.27.2",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^9.12.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "jest": "^29.7.0",
    "jest-fixed-jsdom": "^0.0.11",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.5",
    "typescript": "^5.4.5"
  },
  "jest": {
    "clearMocks": true,
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        "./tsconfig.test.json"
      ]
    },
    "testRegex": "(js|tests)/.*\\.test\\.tsx?$",
    "modulePaths": [
      "<rootDir>/js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "^.+\\.(css|less|scss)$": "<rootDir>/tests/__mocks__/styleMock.js",
      "^Components(.*)$": "<rootDir>/js/components/Dashboard$1",
      "^Hooks(.*)$": "<rootDir>/js/hooks$1",
      "^Models(.*)$": "<rootDir>/js/models$1",
      "^Utils(.*)$": "<rootDir>/js/utils$1",
      "^Constants(.*)$": "<rootDir>/js/constants$1"
    },
    "collectCoverage": true,
    "globalSetup": "./tests/global-setup.js",
    "testEnvironment": "jest-fixed-jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ]
  }
}
